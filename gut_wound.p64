				rect(cont_icn_b_s_x, cont_icn_b_s_y, cont_icn_b_e_x, cont_icn_b_e_y, 2)
				spr(cc.small_icon, cont_icn_x, cont_icn_y)
				for j, ccc in ipairs(cc.contents) do
